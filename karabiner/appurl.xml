	<vkopenurldef>
		<name>KeyCode::VK_OPEN_URL_SHELL_date_pbcopy</name>
		<url type="shell">
		  <![CDATA[
		    /bin/date | /usr/bin/pbcopy;
		    /usr/bin/osascript -e 'display notification "pasteboard has been updated." with title "Karabiner"';
		  ]]>
		</url>
	</vkopenurldef>

	<vkopenurldef>
		<name>KeyCode::VK_OPEN_URL_SHELL_eject_disc_external</name>
		<url type="shell">
		  <![CDATA[
	           /usr/bin/osascript -e 'display notification "Ejecting disc" with title "Karabiner"';
	           (/usr/bin/drutil -drive external status | /usr/bin/grep 'Type: No Media Inserted' > /dev/null) || /usr/bin/drutil -drive external eject
		  ]]>
		</url>
	</vkopenurldef>

	<vkopenurldef>
		<name>KeyCode::VK_OPEN_URL_SHELL_eject_volumes</name>
		<url type="shell">
		  <![CDATA[
	           /usr/bin/osascript -e 'display notification "Ejecting volumes" with title "Karabiner"';
	           for f in /Volumes/*; do /usr/sbin/diskutil unmount "$f"; done
		  ]]>
		</url>
	</vkopenurldef>

	<vkopenurldef>
		<name>KeyCode::VK_OPEN_URL_SHELL_toggle_bluetooth</name>
		<url type="shell">
		  <![CDATA[
			   /usr/bin/osascript -e 'display notification "Toggle Bluetooth" with title "Karabiner"';
			   /Applications/Karabiner.app/Contents/Library/vendor/bin/blueutil toggle
		  ]]>
		</url>
	</vkopenurldef>

	<vkopenurldef>
		<name>KeyCode::VK_OPEN_URL_SHELL_toggle_wifi</name>
		<url type="shell">
		  <![CDATA[
		    CURRENT_DEVICE=`/usr/sbin/networksetup -listallhardwareports | /usr/bin/awk '$3=="Wi-Fi" {getline; print $2}'`;
		    if [ -z "$CURRENT_DEVICE" ]; then
		      /usr/bin/osascript -e 'display notification "No Wi-Fi device" with title "Karabiner"';
		    else
		      if /usr/sbin/networksetup -getairportpower $CURRENT_DEVICE | /usr/bin/grep ': On'; then
		        /usr/sbin/networksetup -setairportpower $CURRENT_DEVICE off
		        /usr/bin/osascript -e 'display notification "Wi-Fi Off" with title "Karabiner"';
		      else
		        /usr/sbin/networksetup -setairportpower $CURRENT_DEVICE on
		        /usr/bin/osascript -e 'display notification "Wi-Fi On" with title "Karabiner"';
		      fi
		    fi
		  ]]>
		</url>
	</vkopenurldef>
